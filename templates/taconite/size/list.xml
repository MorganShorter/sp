<taconite>
    <remove select="#size_list_result tbody tr" />
    <append select="#size_list_result tbody">
        {% for obj in object_list %}
            <tr class="form_list_row" cid="{{ obj.pk }}">
                <td>{{ obj }}</td>
            </tr>
        {% empty %}
            <tr class="form_list_row">
                <td>not found</td>
            </tr>
        {% endfor %}
    </append>
</taconite>